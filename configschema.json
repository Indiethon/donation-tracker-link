{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {
        "apiUrl": "https://example.com/api",
        "token": ""
    },
    "title": "Root Schema",
    "required": [
        "apiUrl",
        "token"
    ],
    "properties": {
        "apiUrl": {
            "type": "string",
            "default": "",
            "title": "The apiUrl Schema",
            "examples": [
                "https://tracker.indiethon.live/api"
            ]
        },
        "token": {
            "type": "string",
            "default": "",
            "title": "The token Schema",
            "examples": [
                "fhdsabuiasfbasuidfbausdif"
            ]
        }
    },
    "examples": [
        {
            "apiUrl": "https://tracker.indiethon.live/api",
            "token": "fhdsabuiasfbasuidfbausdif"
        }
    ]
}